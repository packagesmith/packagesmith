{
  "name": "packagesmith",
  "version": "0.0.0-development",
  "description": "Provisioning/scaffolding tool for Rapid Development",
  "homepage": "http://packagesmith.github.io/packagesmith",
  "bugs": "http://github.com/packagesmith/packagesmith/issues",
  "license": "MIT",
  "author": "Keith Cirkel (http://keithcirkel.co.uk)",
  "files": [
    "lib/*.js"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/packagesmith/packagesmith.git"
  },
  "scripts": {
    "lint": "eslint src test --ignore-path .gitignore",
    "prepublish": "babel src -d lib",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha --recursive test/ --compilers js:babel/register",
    "watch": "npm run prepublish -- -w"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm test"
    }
  },
  "babel": {
    "compact": false,
    "ignore": "node_modules",
    "loose": "all",
    "optional": "runtime",
    "retainLines": true,
    "stage": 2
  },
  "eslintConfig": {
    "extends": "strict",
    "parser": "babel-eslint"
  },
  "dependencies": {
    "babel-runtime": "^5.8.34",
    "chalk": "^1.1.1",
    "child-process-promise": "^1.1.0",
    "debug": "^2.2.0",
    "diff": "^2.2.1",
    "fs-promise": "^0.3.1",
    "ini": "^1.3.4",
    "inquirer": "^0.11.1",
    "lodash.isfunction": "^3.0.6",
    "lodash.kebabcase": "^3.0.1",
    "lodash.partial": "^3.1.1",
    "lodash.pick": "^3.1.0",
    "lodash.pluck": "^3.1.2",
    "lodash.union": "^3.1.0",
    "lodash.uniq": "^3.2.2",
    "lodash.zipobject": "^3.0.0",
    "mkdirp-promise": "^1.0.3"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "chai": "^3.3.0",
    "chai-spies": "^0.7.1",
    "eslint": "^1.10.3",
    "eslint-config-strict": "^7.0.4",
    "eslint-plugin-filenames": "^0.2.0",
    "ghooks": "^1.0.1",
    "mocha": "^2.3.4",
    "semantic-release": "^4.3.5",
    "travis-after-all": "^1.4.4",
    "validate-commit-msg": "^1.0.0"
  }
}